<% @parts.each do |part| %>
  <div id="parts-index">
    <div class="card shadow">
      <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Name: <%= part.title%></h6>
      </div>
      <div class="card-body">

      <div>
        Description: <%= part.description %>
      </div>

      <div>
        Category: <%= part.category %>
      </div>

      </div>
    </div>
  </div>
<% end %>



