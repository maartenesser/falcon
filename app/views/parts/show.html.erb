<div class="container">
  <div class="card shadow mb-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary"><%= @part.title %></h6>
    </div>
    <div class="card-body">
      <ul>
      <li>Current_user.id <%= current_user.id %></li>
      <li>Current_user.email <%= current_user.email %></li>
      <br>
      <li><%= @part.title %></li>
      <li><%= @part.description %></li>
      <li><%= @part.part_model_code %></li>
      <br>
      <li>part.id <%= @part.id %></li>
      <li>part.user_id <%= @part.user_id %></li>
      <li><% if @part.user_id == current_user.id  %>
        my part
        <% end %>
      </li>
      <li>part.claim_id <%= @part.claim_id %></li>
      <li>part.order_id <%= @part.order_id %></li>
      <% if !@order.blank? %>
        <li>order.id <%= @order.id %></li>
      <% end %>
      <!-- <li><%#= @part.price_cents %></li> -->
      </ul>
      <%= button_to "Add to basket", order_part_path(@part), method: :patch %>
    </div>
  </div>



</div>


